{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "id": 15,
            "name": "My Name - Brijendra",
            "releaseDate": "2018-04-01",
            "reviewScore": 99,
            "category": "Shooter",
            "rating": "Mature"
        }
    ],
    "required": [
        "id",
        "name",
        "releaseDate",
        "reviewScore",
        "category",
        "rating"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                15
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "My Name - Brijendra"
            ]
        },
        "releaseDate": {
            "$id": "#/properties/releaseDate",
            "type": "string",
            "title": "The releaseDate schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "2018-04-01"
            ]
        },
        "reviewScore": {
            "$id": "#/properties/reviewScore",
            "type": "integer",
            "title": "The reviewScore schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                99
            ]
        },
        "category": {
            "$id": "#/properties/category",
            "type": "string",
            "title": "The category schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Shooter"
            ]
        },
        "rating": {
            "$id": "#/properties/rating",
            "type": "string",
            "title": "The rating schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Mature"
            ]
        }
    },
    "additionalProperties": true
}